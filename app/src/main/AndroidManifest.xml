<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.projetpokemon">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar">
        <activity android:name=".ActivityMagneton"></activity>
        <activity android:name=".ActivityLamantine" />
        <activity android:name=".ActivityGalopa" />
        <activity android:name=".ActivityFlagadoss" />
        <activity android:name=".ActivityDodrio" />
        <activity android:name=".ActivityCanarticho" />
        <activity android:name=".MainPokemonList" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>